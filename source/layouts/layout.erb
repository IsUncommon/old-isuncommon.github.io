<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <title><%= current_page.data.title || "Uncommon" %></title>
    
    <%= stylesheet_link_tag "application" %>
    <script src="//use.typekit.net/lzu5ret.js"></script>
    <script>try{Typekit.load();}catch(e){}</script>
  </head>
  
  <body id="<%= current_page.data.page_id %>" class="<%= current_page.data.page_classes %>">
    <%= yield %>
  </body>
</html>